  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set default values for ShipRocket API testing",
          "if (!pm.environment.get('shipRocketEmail')) {",
          "    pm.environment.set('shipRocketEmail', 'your-shiprocket-email@example.com');",
          "}",
          "if (!pm.environment.get('shipRocketPassword')) {",
          "    pm.environment.set('shipRocketPassword', 'your-shiprocket-password');",
          "}",
          "if (!pm.environment.get('orderId')) {",
          "    pm.environment.set('orderId', 'TEST-' + Date.now());",
          "}",
          "if (!pm.environment.get('orderDate')) {",
          "    const today = new Date();",
          "    const day = String(today.getDate()).padStart(2, '0');",
          "    const month = String(today.getMonth() + 1).padStart(2, '0');",
          "    const year = today.getFullYear();",
          "    pm.environment.set('orderDate', `${year}-${month}-${day}`);",
          "}",
          "if (!pm.environment.get('customerName')) {",
          "    pm.environment.set('customerName', 'Test Customer');",
          "}",
          "if (!pm.environment.get('customerEmail')) {",
          "    pm.environment.set('customerEmail', 'test.customer@example.com');",
          "}",
          "if (!pm.environment.get('customerPhone')) {",
          "    pm.environment.set('customerPhone', '9876543210');",
          "}",
          "if (!pm.environment.get('billingAddress')) {",
          "    pm.environment.set('billingAddress', '123 Test Street');",
          "}",
          "if (!pm.environment.get('billingCity')) {",
          "    pm.environment.set('billingCity', 'Mumbai');",
          "}",
          "if (!pm.environment.get('billingState')) {",
          "    pm.environment.set('billingState', 'Maharashtra');",
          "}",
          "if (!pm.environment.get('billingCountry')) {",
          "    pm.environment.set('billingCountry', 'India');",
          "}",
          "if (!pm.environment.get('billingPincode')) {",
          "    pm.environment.set('billingPincode', '400001');",
          "}",
          "if (!pm.environment.get('productName')) {",
          "    pm.environment.set('productName', 'Test Product');",
