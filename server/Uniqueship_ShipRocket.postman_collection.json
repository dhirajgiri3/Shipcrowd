    "version": "1.0.0"
  },
  "item": [
    {
      "name": "1. Authentication",
      "item": [
        {
          "name": "Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"email\": \"{{shipRocketEmail}}\",\n    \"password\": \"{{shipRocketPassword}}\"\n}"
            },
            "url": {
              "raw": "{{shipRocketBaseUrl}}/auth/login",
              "host": [
                "{{shipRocketBaseUrl}}"
              ],
              "path": [
                "auth",
                "login"
              ]
            },
            "description": "Authenticate with ShipRocket API and obtain access token. The token is valid for 24 hours."
          },
          "response": []
        }
      ],
      "description": "Authentication endpoints for ShipRocket API"
    },
